<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="mybatis.mapper.chat">
	
	<select id="selectAllChatRoomByEmil" parameterType="String" resultType="chatRoomfoVo">
		SELECT c_id, cr.p_id FROM chatInfomation cr LEFT OUTER JOIN product p
		ON cr.p_id=p.p_id
		WHERE email = #{email}
	</select>
	
</mapper>